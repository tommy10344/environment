#!/usr/bin/ruby
# frozen_string_literal: true

xcworkspace = `ls | grep xcworkspace`.lines.first
xcodeproj = `ls | grep xcodeproj`.lines.first

if xcworkspace
  puts "open #{xcworkspace}"
  `open #{xcworkspace}`
elsif xcodeproj
  puts "open #{xcodeproj}"
  `open #{xcodeproj}`
else
  puts "xcworkspace or xcodeproj not found."
end
